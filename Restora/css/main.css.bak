#header {
	position: absolute;
	z-index: 100;
	top: 0;
	left: 0;
	width: 100%;
	padding: 15px 0;
}

#header:before {
	-webkit-transition: opacity .3s ease-in-out, visibility .3s ease-in-out;
	transition: opacity .3s ease-in-out, visibility .3s ease-in-out;
	content: "";
	position: absolute;
	z-index: 1;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #fff;
	opacity: 0;
	visibility: hidden;
}

#header.fixed-position {
	padding: 15px 0;
}

#header.fixed-position:before {
	opacity: 1;
	visibility: visible;
}

#header.fixed-position .logo {
	color: #191919;
}

#header.fixed-position .nav a {
	color: #ff5722;
}

#header.fixed-position .nav a:hover {
	color: #191919;
}

#header .container {
	position: relative;
	z-index: 2;
}

.logo {
	-webkit-transition: color .3s ease-in-out;
	transition: color .3s ease-in-out;
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-ms-flex-align: center;
	align-items: center;
	width: 265px;
	color: #fff;
	text-align: left;
	font-weight: 400;
}

.logo .img-holder {
	width: 60px;
	height: 60px;
}

.logo .brand {
	display: block;
	font: 40px/1.2 "Pacifico", "Times New Roman", "Times", "Baskerville", "Georgia", cursive;
	letter-spacing: 7px;
}

.logo .slogan {
	display: block;
	font-size: 18px;
	letter-spacing: .8px;
}

.logo .text-holder {
	padding-left: 10px;
	width: calc(100% - 60px);
}

.nav {
	-webkit-transition: opacity .3s ease-in-out, visibility .3s ease-in-out;
	transition: opacity .3s ease-in-out, visibility .3s ease-in-out;
	font: 700 18px "Roboto", "Arial", "Helvetica Neue", "Helvetica", sans-serif;
	letter-spacing: 1.4px;
}

.nav a {
	text-transform: uppercase;
	color: #ff5722;
}

.nav a:hover {
	color: #191919;
}

.nav li {
	padding: 0 0 20px;
}

.nav li.active a {
	color: #ff5722;
}

.menu-holder {
	width: calc(100% - 133px);
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: end;
	-ms-flex-pack: end;
	justify-content: flex-end;
	-webkit-box-align: center;
	-ms-flex-align: center;
	align-items: center;
}

.nav-opener {
	position: relative;
	width: 40px;
	height: 30px;
	margin-left: 10px;
}

.nav-opener:hover {
	opacity: .7;
}

.nav-opener:before,
.nav-opener:after,
.nav-opener span {
	-webkit-transition: opacity .3s ease-in-out, background .3s ease-in-out;
	transition: opacity .3s ease-in-out, background .3s ease-in-out;
	background: #ff5722;
	height: 3px;
	position: absolute;
	top: 14px;
	left: 15%;
	right: 15%;
}

.nav-opener:before,
.nav-opener:after {
	content: "";
	top: 3px;
	-webkit-transition: all .3s linear;
	transition: all .3s linear;
}

.nav-opener:after {
	top: 24px;
}

.nav-active .nav-opener {
	background: transparent;
}

.nav-active .nav-opener:after,
.nav-active .nav-opener:before {
	-webkit-transition: -webkit-transform .3s ease-in-out;
	transition: -webkit-transform .3s ease-in-out;
	transition: transform .3s ease-in-out;
	transition: transform .3s ease-in-out, -webkit-transform .3s ease-in-out;
	-webkit-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	transform: rotate(45deg);
	top: 14px;
	left: 12%;
	right: 12%;
}

.nav-active .nav-opener:after {
	-webkit-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg);
	transform: rotate(-45deg);
}

.nav-active .nav-opener span {
	opacity: 0;
}

.nav-active #header:before {
	opacity: 1;
	visibility: visible;
}

.nav-active .logo {
	color: #000;
}

.nav-active .nav {
	opacity: 1;
	visibility: visible;
}

.resize-active * {
	-webkit-transition: none !important;
	transition: none !important;
}

